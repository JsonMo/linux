 server
    {
        listen       80;
        server_name  catalog.cobazaar.com;


        location /
        {
            root   /home/java/pp2s-catalog-web;
            index  index.html index.htm index.jsp;
                        #proxy_pass http://server-pool;
        }


        error_page   500 502 503 504  /50x.html;
        location = /50x.html {
            root   html;
        }

    }
